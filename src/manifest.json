{
  "name": "Send Daily Report",
  "manifest_version": 3,
  "version": "1.1",
  "host_permissions": ["https://*/*"],
  "background": {
    "service_worker": "background.ts"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage"],
  "options_ui": {
    "page": "options.html"
  },
  "icons": {
    "16": "/icons/icon.png"
  }
}
